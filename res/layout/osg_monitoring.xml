<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical"
  android:layout_width="match_parent"
  android:layout_height="match_parent" android:weightSum="1">
    <TableLayout android:layout_width="match_parent" android:layout_height="wrap_content" android:id="@+id/tableLayout1">
        <TableRow android:id="@+id/tableRow1" android:layout_width="wrap_content" android:layout_height="wrap_content">
        </TableRow>
        <TableRow android:id="@+id/tableRow2" android:layout_width="wrap_content" android:layout_height="wrap_content">
            <TextView android:textAppearance="?android:attr/textAppearanceLarge" android:layout_width="wrap_content" android:text="@string/vos" android:id="@+id/textView2" android:layout_height="wrap_content"></TextView>
            <AutoCompleteTextView android:layout_height="wrap_content" android:id="@+id/autoCompleteTextView2" android:text="@string/empty" android:layout_width="100dp"></AutoCompleteTextView>
        </TableRow>
        <TableRow android:id="@+id/tableRow3" android:layout_width="wrap_content" android:layout_height="wrap_content">
            <TextView android:textAppearance="?android:attr/textAppearanceLarge" android:layout_width="wrap_content" android:text="@string/site" android:id="@+id/textView1" android:layout_height="wrap_content"></TextView>
            <AutoCompleteTextView android:layout_weight="1" android:layout_width="match_parent" android:layout_height="wrap_content" android:id="@+id/autoCompleteTextView1" android:text="@string/empty">
                <requestFocus></requestFocus>
            </AutoCompleteTextView>
        </TableRow>
    </TableLayout>
    <Button android:id="@+id/confirm_site" android:text="@string/get_usage" android:layout_width="match_parent" android:layout_height="wrap_content"></Button>
    
</LinearLayout>
