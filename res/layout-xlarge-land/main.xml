<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:weightSum="1">
    <TableLayout android:layout_width="match_parent" android:id="@+id/tableLayout2" android:layout_height="wrap_content" android:layout_weight="1">
        <TableRow android:id="@+id/tableRow2" android:layout_weight="1" android:layout_width="match_parent" android:layout_height="match_parent">
            <WebView android:layout_width="match_parent" android:id="@+id/webView1" android:layout_height="match_parent" android:layout_weight="1"></WebView>
        </TableRow>
        <TableRow android:id="@+id/tableRow3" android:layout_width="wrap_content" android:layout_height="wrap_content">
            <Button android:text="@string/viewmonitoring" android:layout_width="match_parent" android:id="@+id/view_monitoring" android:layout_height="match_parent" android:layout_weight="1"></Button>
            <Button android:text="@string/viewmap" android:layout_width="match_parent" android:id="@+id/view_map" android:layout_height="match_parent" android:layout_weight="1"></Button>
        </TableRow>
    </TableLayout>
</LinearLayout>
