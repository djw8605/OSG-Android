<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.osg.osgmon" android:versionName="1.5" android:versionCode="6">
    <uses-sdk android:minSdkVersion="8" />
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application android:icon="@drawable/icon" android:label="@string/app_name" android:debuggable="true">
        <activity android:name=".HelloAndroid"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="OSGMonitoring"></activity>
        <activity android:name="org.achartengine.GraphicalActivity"></activity>
        <uses-library android:name="com.google.android.maps" />
        <activity android:name="com.osg.osgmon.OSGMapView"></activity>
    </application>
</manifest>