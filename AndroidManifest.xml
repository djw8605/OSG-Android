<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.osg.osgmon" android:versionName="2.2" android:versionCode="17" xmlns:android="http://schemas.android.com/apk/res/android">
      <supports-screens android:resizeable="true"
                  android:smallScreens="true"
                  android:normalScreens="true"
                  android:largeScreens="false" />
    <uses-sdk android:minSdkVersion="8" android:maxSdkVersion="10"/>
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application android:icon="@drawable/icon" android:label="@string/app_name" android:debuggable="true">
        <activity android:name=".HelloAndroid"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".OSGMonitoringActivity"></activity>
        <activity android:name="org.achartengine.GraphicalActivity"></activity>
        <uses-library android:name="com.google.android.maps" />
        <activity android:name="com.osg.osgmon.OSGMapView"></activity>
        <activity android:name="OSGSiteUsage"></activity>
    </application>
</manifest>
